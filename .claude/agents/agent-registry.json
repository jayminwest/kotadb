{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Claude Code Agent Registry",
  "description": "Machine-readable registry of available agents with capability and model indexes",
  "version": "2.0.0",
  "agents": {
    "scout-agent": {
      "name": "scout-agent",
      "description": "Read-only codebase exploration and research agent",
      "file": "scout-agent.md",
      "model": "haiku",
      "capabilities": ["explore", "search", "analyze", "research"],
      "tools": [
        "Glob",
        "Grep",
        "Read",
        "Task",
        "mcp__kotadb__search_code",
        "mcp__kotadb__search_dependencies",
        "mcp__kotadb__list_recent_files"
      ],
      "readOnly": true
    },
    "build-agent": {
      "name": "build-agent",
      "description": "File implementation and modification specialist",
      "file": "build-agent.md",
      "model": "sonnet",
      "capabilities": ["implement", "modify", "execute", "create", "fix"],
      "tools": [
        "Glob",
        "Grep",
        "Read",
        "Edit",
        "Write",
        "Bash",
        "NotebookEdit",
        "Task",
        "TodoWrite",
        "mcp__kotadb__search_code",
        "mcp__kotadb__search_dependencies",
        "mcp__kotadb__analyze_change_impact"
      ],
      "readOnly": false
    },
    "review-agent": {
      "name": "review-agent",
      "description": "Code review and quality analysis specialist",
      "file": "review-agent.md",
      "model": "haiku",
      "capabilities": ["review", "audit", "quality", "security", "feedback"],
      "tools": [
        "Glob",
        "Grep",
        "Read",
        "Task",
        "mcp__kotadb__search_code",
        "mcp__kotadb__search_dependencies",
        "mcp__kotadb__analyze_change_impact",
        "WebFetch"
      ],
      "readOnly": true
    },
    "docs-scraper": {
      "name": "docs-scraper",
      "description": "Documentation scraping specialist for fetching and saving docs as markdown",
      "file": "docs-scraper.md",
      "model": "sonnet",
      "capabilities": ["scrape", "fetch", "document"],
      "tools": [
        "mcp__firecrawl-mcp__firecrawl_scrape",
        "WebFetch",
        "Write",
        "Edit"
      ],
      "readOnly": false
    },
    "claude-config-plan-agent": {
      "name": "claude-config-plan-agent",
      "description": "Claude configuration planning and spec generation specialist",
      "file": "experts/claude-config/claude-config-plan-agent.md",
      "model": "sonnet",
      "capabilities": ["claude-config-planning", "spec-generation"],
      "tools": ["Read", "Glob", "Grep", "Write"],
      "readOnly": false
    },
    "claude-config-build-agent": {
      "name": "claude-config-build-agent",
      "description": "Claude configuration implementation specialist",
      "file": "experts/claude-config/claude-config-build-agent.md",
      "model": "sonnet",
      "capabilities": ["claude-config-implementation"],
      "tools": ["Read", "Write", "Edit", "Glob", "Grep"],
      "readOnly": false
    },
    "claude-config-improve-agent": {
      "name": "claude-config-improve-agent",
      "description": "Claude configuration expertise evolution specialist",
      "file": "experts/claude-config/claude-config-improve-agent.md",
      "model": "sonnet",
      "capabilities": ["claude-config-expertise-evolution"],
      "tools": ["Read", "Write", "Edit", "Glob", "Grep", "Bash"],
      "readOnly": false
    },
    "claude-config-question-agent": {
      "name": "claude-config-question-agent",
      "description": "Claude configuration Q&A specialist",
      "file": "experts/claude-config/claude-config-question-agent.md",
      "model": "haiku",
      "capabilities": ["claude-config-qa"],
      "tools": ["Read", "Glob", "Grep"],
      "readOnly": true
    },
    "agent-authoring-plan-agent": {
      "name": "agent-authoring-plan-agent",
      "description": "Agent authoring planning and spec generation specialist",
      "file": "experts/agent-authoring/agent-authoring-plan-agent.md",
      "model": "sonnet",
      "capabilities": ["agent-authoring-planning", "spec-generation"],
      "tools": ["Read", "Glob", "Grep", "Write"],
      "readOnly": false
    },
    "agent-authoring-build-agent": {
      "name": "agent-authoring-build-agent",
      "description": "Agent authoring implementation specialist",
      "file": "experts/agent-authoring/agent-authoring-build-agent.md",
      "model": "sonnet",
      "capabilities": ["agent-authoring-implementation"],
      "tools": ["Read", "Write", "Edit", "Glob", "Grep"],
      "readOnly": false
    },
    "agent-authoring-improve-agent": {
      "name": "agent-authoring-improve-agent",
      "description": "Agent authoring expertise evolution specialist",
      "file": "experts/agent-authoring/agent-authoring-improve-agent.md",
      "model": "sonnet",
      "capabilities": ["agent-authoring-expertise-evolution"],
      "tools": ["Read", "Write", "Edit", "Glob", "Grep", "Bash"],
      "readOnly": false
    },
    "agent-authoring-question-agent": {
      "name": "agent-authoring-question-agent",
      "description": "Agent authoring Q&A specialist",
      "file": "experts/agent-authoring/agent-authoring-question-agent.md",
      "model": "haiku",
      "capabilities": ["agent-authoring-qa"],
      "tools": ["Read", "Glob", "Grep"],
      "readOnly": true
    }
  },
  "capabilityIndex": {
    "explore": ["scout-agent"],
    "search": ["scout-agent"],
    "analyze": ["scout-agent", "review-agent"],
    "research": ["scout-agent"],
    "implement": ["build-agent"],
    "modify": ["build-agent"],
    "execute": ["build-agent"],
    "create": ["build-agent"],
    "fix": ["build-agent"],
    "review": ["review-agent"],
    "audit": ["review-agent"],
    "quality": ["review-agent"],
    "security": ["review-agent"],
    "feedback": ["review-agent"],
    "scrape": ["docs-scraper"],
    "fetch": ["docs-scraper"],
    "document": ["docs-scraper"],
    "claude-config-planning": ["claude-config-plan-agent"],
    "claude-config-implementation": ["claude-config-build-agent"],
    "claude-config-expertise-evolution": ["claude-config-improve-agent"],
    "claude-config-qa": ["claude-config-question-agent"],
    "agent-authoring-planning": ["agent-authoring-plan-agent"],
    "agent-authoring-implementation": ["agent-authoring-build-agent"],
    "agent-authoring-expertise-evolution": ["agent-authoring-improve-agent"],
    "agent-authoring-qa": ["agent-authoring-question-agent"],
    "spec-generation": ["claude-config-plan-agent", "agent-authoring-plan-agent"]
  },
  "modelIndex": {
    "haiku": ["scout-agent", "review-agent", "claude-config-question-agent", "agent-authoring-question-agent"],
    "sonnet": ["build-agent", "docs-scraper", "claude-config-plan-agent", "claude-config-build-agent", "claude-config-improve-agent", "agent-authoring-plan-agent", "agent-authoring-build-agent", "agent-authoring-improve-agent"]
  },
  "toolMatrix": {
    "Glob": ["scout-agent", "build-agent", "review-agent", "claude-config-plan-agent", "claude-config-build-agent", "claude-config-improve-agent", "claude-config-question-agent", "agent-authoring-plan-agent", "agent-authoring-build-agent", "agent-authoring-improve-agent", "agent-authoring-question-agent"],
    "Grep": ["scout-agent", "build-agent", "review-agent", "claude-config-plan-agent", "claude-config-build-agent", "claude-config-improve-agent", "claude-config-question-agent", "agent-authoring-plan-agent", "agent-authoring-build-agent", "agent-authoring-improve-agent", "agent-authoring-question-agent"],
    "Read": ["scout-agent", "build-agent", "review-agent", "claude-config-plan-agent", "claude-config-build-agent", "claude-config-improve-agent", "claude-config-question-agent", "agent-authoring-plan-agent", "agent-authoring-build-agent", "agent-authoring-improve-agent", "agent-authoring-question-agent"],
    "Edit": ["build-agent", "docs-scraper", "claude-config-build-agent", "claude-config-improve-agent", "agent-authoring-build-agent", "agent-authoring-improve-agent"],
    "Write": ["build-agent", "docs-scraper", "claude-config-plan-agent", "claude-config-build-agent", "claude-config-improve-agent", "agent-authoring-plan-agent", "agent-authoring-build-agent", "agent-authoring-improve-agent"],
    "Bash": ["build-agent", "claude-config-improve-agent", "agent-authoring-improve-agent"],
    "NotebookEdit": ["build-agent"],
    "Task": ["scout-agent", "build-agent", "review-agent"],
    "TodoWrite": ["build-agent"],
    "WebFetch": ["review-agent", "docs-scraper"],
    "mcp__kotadb__search_code": ["scout-agent", "build-agent", "review-agent"],
    "mcp__kotadb__search_dependencies": ["scout-agent", "build-agent", "review-agent"],
    "mcp__kotadb__list_recent_files": ["scout-agent"],
    "mcp__kotadb__analyze_change_impact": ["build-agent", "review-agent"],
    "mcp__firecrawl-mcp__firecrawl_scrape": ["docs-scraper"]
  }
}
